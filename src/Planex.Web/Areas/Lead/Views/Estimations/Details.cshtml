@model Planex.Web.Areas.Lead.Models.EstimationRequestedViewModel

<div class="k-content">
    <div>
        <h4>EstimationRequestedViewModel</h4>
        <hr />

        @Html.DisplayFor(model => model.Title)
        @Html.DisplayFor(model => model.Description)
        @Html.DisplayFor(model => model.Priority)

        @foreach (var file in Model.UploadedAttachmentFiles)
        {
            <a href="@file">@file</a>
        }

    </div>
    <p>
        @Html.ActionLink("Start Estimation", "StartEstimation", new { id = Model.Id }) |
    </p>
</div>
